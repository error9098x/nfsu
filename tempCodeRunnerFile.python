def prime(x):
    l=0
    for i in range(1,x):
        if x%i==0:
            l+=1
    if l==1:
        return True
    else:
        return False
        
def Twin_Primes(n,m):
    l=[]
    for i in range(n,m-1):
        if prime(i) and prime(i+2):
            l.append(n,m)
    print(l)
